{{#each this}}
<h2 class="country-title">{{name}}</h2>
<div class="card">
    <div class="card-text">
        <p class="card-text"><b>Capital: </b>{{capital}}</p>
        <p class="card-text"><b>Population: </b> {{population}}</p>
        <p class="card-text"><b>Languages: </b></p>
        <ul class="list language-list">
            {{#each languages}}
                <li>{{this.name}}</li>
            {{/each}}
        </ul>
    </div>
    <div class="card-img">
        <img class="flag" src="{{flag}}" alt="{{name}} flag">
    </div>
</div>
{{/each}}